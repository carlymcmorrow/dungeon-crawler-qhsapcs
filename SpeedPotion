public class SpeedPotion extends Item{
	
	private int size;
	private int price;
	private int currentCash;
		
	public SpeedPotion(int size2, int price2){
		size = size2;
		price = price2;
		currentCash = this.getCash;
	}
		
	public void addToSpeed(){
		this.setSpeed(this.getSpeed() + size);
	}
		
	public void changeCash(int inputCash){
		currentCash = currentCash - inputCash; 
	}

	public void pickUpItem() {
		// TODO Auto-generated method stub
		
	}

	public void dropItem() {
		// TODO Auto-generated method stub
		
	}
	
}
